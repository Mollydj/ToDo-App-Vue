sContent":["<template>\n  <div class=\"container\">\n    <div class=\"todos\">\n      <md-field>\n        <label>Jot it down...</label>\n        <md-input\n          v-model=\"currentTodo\"\n          class=\"todo-item form-control input-height\"\n          type=\"text\"\n          @keydown.enter=\"addTodo()\"\n        >\n        </md-input>\n      </md-field>\n\n      <div v-for=\"todo in todos\" :key=\"todo.id\">\n        <span v-if=\"!todo.editing\">\n          {{ todo.label }}\n        </span>\n\n        <span v-if=\"!todo.editing\" @dblclick=\"todo.editing = true\">\n          {{ todo.description }}\n          <button\n            type=\"button\"\n            @click=\"todo.editing = true\"\n            class=\"btn btn-default \"\n          >\n            Edit\n          </button>\n          <button @click=\"removeTodo(todo)\">Delete</button>\n          <input type=\"checkbox\" id=\"checkbox\" v-model=\"boolean\" />\n        </span>\n\n        <md-field>\n          <md-input\n            v-model=\"todo.label\"\n            v-if=\"todo.editing\"\n            @keyup.enter=\"edittodo(todo)\"\n            type=\"text\"\n            class=\"todo-item form-control input-height\"\n          >\n          </md-input>\n        </md-field>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      todos: [],\n      currentTodo: \"\",\n      editTodo: \"\",\n      boolean: false,\n    };\n  },\n  methods: {\n    addTodo() {\n      this.todos.push({\n        id: this.todos.length,\n        label: this.currentTodo,\n        completed: false,\n        editing: false,\n      });\n      this.currentTodo = \"\";\n    },\n    removeTodo(todo) {\n      var index = this.todos.indexOf(todo);\n      this.todos.splice(index, 1);\n    },\n    edittodo: function(todo) {\n      todo.editing = false;\n      console.log(todo);\n    },\n    completetodo: function(todo) {\n      todo.completed = true;\n      console.log(todo.completed);\n    },\n  },\n};\n</script>\n\n<style>\n/* You can add global styles to this file, and also import other style files */\nbody {\n  font-family: Roboto, Arial, sans-serif;\n  background-color: antiquewhite;\n}\n\n.container {\n  width: 70%;\n  font-family: Roboto, Arial, sans-serif;\n  margin: auto;\n  display: block;\n}\ntodo-item {\n  justify-content: left;\n  width: 100%;\n  display: block;\n  border: none;\n}\n\n.todo-row,\n.md-field {\n  display: block;\n  color: red;\n}\n</style>\n"],"sourceRoot":"src"}]}